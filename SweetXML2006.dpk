{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'CA SweetControls'}
{$IMPLICITBUILD OFF}

requires
	vcl,
	rtl,
	designide,
	vclactnband,
	vclx,
	dbrtl,
	vcldb;

contains
	SCUnicodeLib in 'SCUnicodeLib.pas',
	SCUnicodeLibRes in 'SCUnicodeLibRes.pas',
	SCXMLRes in 'SCXMLRes.pas',
	SCXML in 'SCXML.pas';

end.