{$DESCRIPTION 'CA Sweet XML'}
{$IMPLICITBUILD ON}

requires
  vcl50;

contains
  SCUnicodeLib in 'SCUnicodeLib.pas',
  SCUnicodeLibRes in 'SCUnicodeLibRes.pas',
  SCXMLRes in 'SCXMLRes.pas',
  SCXML in 'SCXML.pas',
  SweetXMLRegs in 'SweetXMLRegs.pas';

end.
